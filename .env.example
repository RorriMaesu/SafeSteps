# Copy this file to .env and fill in the values. Do NOT commit your .env.

# Google Maps SDK for Android key (enable "Maps SDK for Android" in GCP)
GOOGLE_MAPS_API_KEY=

# Firebase Web client ID for Google Sign-In (from Firebase console > Authentication > Sign-in method)
GOOGLE_WEB_CLIENT_ID=

# Android package name (must match Firebase app and GCP API restrictions)
ANDROID_PACKAGE_NAME=com.safesteps.app
